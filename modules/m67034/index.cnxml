              </m:mrow>
            </m:mrow>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{ { bar  {v}}= {  {v rSub { size 8{0} } +v}  over  {2} } } {}</m:annotation>
        </m:semantics>
      </m:math>
    </equation>
    <equation id="import-auto-id2175249">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mrow>
              <m:mrow>
                <m:mi>v</m:mi>
                <m:mo stretchy="false">=</m:mo>
                <m:mrow>
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mrow>
